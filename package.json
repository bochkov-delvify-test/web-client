{
    "name": "todo-web",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "prettier --check . && eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint-fix": "prettier --write . && eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
        "preview": "vite preview",
        "prepare": "husky install"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@mantine/core": "^6.0.16",
        "@mantine/dates": "^6.0.16",
        "@mantine/form": "^6.0.16",
        "@mantine/hooks": "^6.0.16",
        "@mantine/notifications": "^6.0.16",
        "@svgr/webpack": "^8.0.1",
        "@tabler/icons-react": "^2.24.0",
        "dayjs": "^1.11.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.14",
        "@types/react-beautiful-dnd": "^13.1.4",
        "@types/react-dom": "^18.2.6",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.1",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.3",
        "prettier": "3.0.0",
        "typescript": "^5.0.2",
        "vite": "^4.4.0"
    }
}
